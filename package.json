    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "autoprefixer": "^10.4.18",
    "axios": "^1.6.8",
